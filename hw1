CREATE TABLE IF NOT EXISTS Genre (
	id SERIAL PRIMARY KEY,
	genreName VARCHAR(40) NOT NULL
);

CREATE TABLE IF NOT EXISTS Artists (
	id INTEGER PRIMARY KEY ,
	artistsName VARCHAR(120)
);
CREATE TABLE IF NOT EXISTS Genre_artists (
	id SERIAL PRIMARY KEY,
	genre_id INTEGER NOT NULL REFERENCES Genre(id),
	artist_id INTEGER NOT NULL REFERENCES Artists(id)
	
);
CREATE TABLE IF NOT EXISTS Albums (
	id SERIAL PRIMARY KEY,
	albumName VARCHAR(160) NOT NULL,
	year INTEGER NOT NULL
);
CREATE TABLE IF NOT EXISTS Artist_albums (
	id SERIAL PRIMARY KEY,
	artists_id INTEGER NOT NULL REFERENCES Artists(id),
	albums_id INTEGER NOT NULL REFERENCES Albums(id)
	
);
CREATE TABLE IF NOT EXISTS MusicTrack (
	id SERIAL PRIMARY KEY,
	trackNAME VARCHAR(180) NOT NULL,
	albums INTEGER NOT NULL REFERENCES Albums(id)
	 
);

CREATE TABLE IF NOT EXISTS Collections (
	id SERIAL PRIMARY KEY,
	name VARCHAR(180) NOT NULL,
	year INTEGER NOT NULL 
	
);
CREATE TABLE IF NOT EXISTS MusicTrack_Collections (
	id SERIAL PRIMARY KEY,
	music_id INTEGER NOT NULL REFERENCES MusicTrack(id),
	collection_id INTEGER NOT NULL REFERENCES Collections(id)
	
);
